<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="br.com.bancoamazonia.integracao.card.model.domain.Movimento" table="movimento">
		<composite-id>
			<key-property name="cartao" column="numero_cartao"/>
			<key-property name="evento" column="codigo_evento"/>
			<key-property name="sequencial" type="integer"/>
		</composite-id>
		<property name="data"/>
		<property name="agencia"/>
		<property name="valido"/>
		<many-to-one name="arquivoOrigem" column="arquivo_origem" class="br.com.bancoamazonia.integracao.card.model.domain.Arquivo"/>
	</class>
</hibernate-mapping>